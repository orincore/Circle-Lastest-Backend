{
  "name": "circle-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "vercel-build": "npm run build",
    "render-build": "npm install && npm run build",
    "start:optimized": "node dist/server/app.js",
    "start:worker": "node dist/server/workers/matchmaking-worker.js",
    "start:cluster": "pm2 start ecosystem.config.js",
    "redis:start": "docker-compose up -d redis",
    "redis:stop": "docker-compose stop redis",
    "redis:cli": "redis-cli",
    "health": "curl http://localhost:3000/health",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@aws-sdk/client-s3": "^3.665.0",
    "@aws-sdk/s3-request-presigner": "^3.665.0",
    "@supabase/supabase-js": "^2.49.1",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "redis": "^4.6.14",
    "socket.io": "^4.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.4",
    "@types/pino-http": "^5.8.4",
    "@types/socket.io": "^3.0.2",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "pino-pretty": "^11.2.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
